{
  "name": "lucia-monge-portfolio",
  "version": "1.0.0",
  "description": "Kirby + Laravel Mix site for Lucia Monge",
  "main": "src/script.js",
  "scripts": {
    "build": "node esbuild.build.mjs",
    "watch": "node esbuild.watch.mjs",
    "browsersync": "browser-sync start --https --proxy 'https://lucia-monge-portfolio.test' --files 'assets/**/*.css' 'site/**/*.php' 'site/**/**/*.php' --no-open --no-notify --no-inject-changes",
    "dev": "npm run watch & npm run browsersync",
    "push-content": "rsync -aziv --delete --exclude='*.DS_Store' ./content/ dh_k26cc3@iad1-shared-e1-28.dreamhost.com:lucmon15.dreamhosters.com/content/",
    "push-content-dry": "rsync -anziv --delete --exclude='*.DS_Store' ./content/ dh_k26cc3@iad1-shared-e1-28.dreamhost.com:lucmon15.dreamhosters.com/content/",
    "push-vendor": "rsync -aziv --delete --exclude='*.DS_Store' ./vendor/ dh_k26cc3@iad1-shared-e1-28.dreamhost.com:lucmon15.dreamhosters.com/vendor/",
    "push-vendor-dry": "rsync -anziv --delete --exclude='*.DS_Store' ./vendor/ dh_k26cc3@iad1-shared-e1-28.dreamhost.com:lucmon15.dreamhosters.com/vendor/",
    "push-assets": "rsync -aziv --delete --exclude='*.DS_Store' ./assets/ dh_k26cc3@iad1-shared-e1-28.dreamhost.com:lucmon15.dreamhosters.com/assets/",
    "push-assets-dry": "rsync -anziv --delete --exclude='*.DS_Store' ./assets/ dh_k26cc3@iad1-shared-e1-28.dreamhost.com:lucmon15.dreamhosters.com/assets/",
    "push-kirby": "rsync -aziv --delete --exclude='*.DS_Store' ./kirby/ dh_k26cc3@iad1-shared-e1-28.dreamhost.com:lucmon15.dreamhosters.com/kirby/",
    "push-kirby-dry": "rsync -anziv --delete --exclude='*.DS_Store' ./kirby/ dh_k26cc3@iad1-shared-e1-28.dreamhost.com:lucmon15.dreamhosters.com/kirby/",
    "push-templates": "rsync -aziv --delete --exclude='*.DS_Store' --exclude='accounts' --exclude='cache' --exclude='config' --exclude='plugins' --exclude='sessions' ./site/ dh_k26cc3@iad1-shared-e1-28.dreamhost.com:lucmon15.dreamhosters.com/site/",
    "push-templates-dry": "rsync -anziv --delete --exclude='*.DS_Store' --exclude='accounts' --exclude='cache' --exclude='config' --exclude='plugins' --exclude='sessions' ./site/ dh_k26cc3@iad1-shared-e1-28.dreamhost.com:lucmon15.dreamhosters.com/site/",
    "pull-content": "rsync -aziv --delete dh_k26cc3@iad1-shared-e1-28.dreamhost.com:lucmon15.dreamhosters.com/content/ ./content/",
    "pull-content-dry": "rsync -anziv --delete dh_k26cc3@iad1-shared-e1-28.dreamhost.com:lucmon15.dreamhosters.com/content/ ./content/",
    "pull-assets": "rsync -aziv --delete dh_k26cc3@iad1-shared-e1-28.dreamhost.com:lucmon15.dreamhosters.com/assets/ ./assets/",
    "pull-assets-dry": "rsync -anziv --delete dh_k26cc3@iad1-shared-e1-28.dreamhost.com:lucmon15.dreamhosters.com/assets/ ./assets/"
  },
  "keywords": [],
  "author": "Mrityunjay Marol",
  "license": "ISC",
  "devDependencies": {
    "autoprefixer": "^10.4.19",
    "browser-sync": "^3.0.2",
    "browserslist": "^4.23.0",
    "browserslist-to-esbuild": "^2.1.1",
    "cssnano": "^6.1.2",
    "esbuild": "^0.20.2",
    "fslightbox": "^3.4.1",
    "parcel": "^2.12.0",
    "postcss": "^8.4.38",
    "postcss-cli": "^11.0.0",
    "postcss-import": "^16.1.0",
    "postcss-nesting": "^12.1.1"
  }
}
